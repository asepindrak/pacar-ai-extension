{
  "name": "pacar-ai",
  "displayName": "pacar-ai",
  "description": "Pacar AI** is a chat extension that assists you while programming. With this AI companion, you can enjoy roleplaying, emotional support, and smart insights to enhance your coding sessions. Never code alone againâ€”experience coding with a touch of companionship! ðŸŒŸðŸ’»ðŸ’•",
  "version": "1.2.1",
  "publisher": "PacarAI",
  "repository": {
    "type": "git",
    "url": "https://github.com/asepindrak/pacar-ai-extension.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "media/icon.png",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pacar-ai-sidebar",
          "title": "Pacar AI",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "pacar-ai-sidebar": [
        {
          "type": "webview",
          "id": "pacar-ai-sidebar",
          "name": "Pacar AI",
          "icon": "media/icon.svg",
          "contextualTitle": "PacarAI"
        }
      ]
    },
    "activationEvents": [
      "onView:pacar-ai-sidebar"
    ],
    "commands": [
      {
        "command": "pacar-ai.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "pacar-ai.applyCode",
        "title": "Apply Code Completion"
      },
      {
        "command": "pacar-ai.triggerTab",
        "title": "Trigger Tab for Code Completion"
      }
    ],
    "keybindings": [
      {
        "command": "pacar-ai.triggerCompletion",
        "key": "ctrl+enter",
        "when": "editorTextFocus"
      },
      {
        "key": "tab",
        "command": "pacar-ai.triggerTab",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}