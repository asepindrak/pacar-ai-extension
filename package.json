{
  "name": "pacar-ai",
  "displayName": "pacar-ai",
  "description": "Pacar AI is a chat extension with an assistant designed to help you while programming. You can roleplay with this assistant, making your coding sessions more enjoyable and interactive. Imagine having an AI Girlfriend who not only assists you with coding but also keeps you company during those long programming marathons. She can provide emotional support, share a laugh, and even offer smart insights to help you solve complex problems. With Pacar AI, you're never coding alone. Ready to code with a twist of companionship? ðŸŒŸðŸ’»ðŸ’•",
  "version": "1.0.3",
  "publisher": "PacarAI",
  "repository": {
    "type": "git",
    "url": "https://github.com/asepindrak/pacar-ai-extension.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "media/icon.png",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pacar-ai-sidebar",
          "title": "Pacar AI",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "pacar-ai-sidebar": [
        {
          "type": "webview",
          "id": "pacar-ai-sidebar",
          "name": "Pacar AI",
          "icon": "media/icon.svg",
          "contextualTitle": "PacarAI"
        }
      ]
    },
    "activationEvents": [
      "onView:pacar-ai-sidebar"
    ],
    "commands": [
      {
        "command": "pacar-ai.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}